
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.VBox?>
<StackPane xmlns="http://javafx.com/javafx/23.0.1"
    xmlns:fx="http://javafx.com/fxml/1"
    fx:controller="dot.javaFX.controller.ScannReceiptBtnController" alignment="CENTER" fx:id="container">
    <Button fx:id="scannReceiptBtn" mnemonicParsing="false"
        onAction="#handelScannReceiptBtn" text="Kassenbon scannen" alignment="CENTER" visible="true">
        <VBox.margin>
            <Insets bottom="8.0" top="8.0" />
        </VBox.margin>
    </Button>
<ProgressIndicator fx:id="progressIndicator" maxHeight="25" maxWidth="25" visible="false"/>
</StackPane>